{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "linebreak-style": "off",
    "lines-between-class-members": "off",
		"prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
